        "socket.io": "^4.8.1",
        "winston": "^3.16.0"
    },
    "devDependencies": {
        "axios": "^1.7.7",
        "jest": "^29.7.0",
        "nodemon": "^2.0.22",
        "sequelize-cli": "^6.6.2"
