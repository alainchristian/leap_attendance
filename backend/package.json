{
    "name": "asyv-leap-backend",
    "version": "1.0.0",
    "description": "ASYV LEAP EP Management System Backend",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "migrate": "sequelize-cli db:migrate",
        "seed": "sequelize-cli db:seed:all",
        "db:init": "node src/utils/init-db.js",
